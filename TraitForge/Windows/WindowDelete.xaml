<Window x:Class="TraitForge.Windows.WindowDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TraitForge.Windows"
        mc:Ignorable="d"
        Title="WindowDelete" Height="160" Width="380" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Are you sure you want to delete the object?" HorizontalAlignment="Left" Margin="65,35,0,0" VerticalAlignment="Top" Width="251"/>
        <Button x:Name="BtnConfirm" Content="Confirm" HorizontalAlignment="Left" Margin="209,84,0,0" VerticalAlignment="Top" Click="BtnConfirm_Click"/>
        <Button x:Name="BtnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="112,84,0,0" VerticalAlignment="Top" Click="BtnCancel_Click" RenderTransformOrigin="0.401,0.583"/>

    </Grid>
</Window>
